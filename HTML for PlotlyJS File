<body>
  
  <div id="myDiv" style="width: 1200px; height:800px;"></div>
  <script>
      var trace2 = {
  x: [1,2,3,4], 
  y:[3,6,8,9], 
  type: 'line'
};
    var trace3 = {
    x: [1.2,3,4], 
    y: [2,4,6,8], 
    type: 'line'
};
      var layout = {
  title: 'Youtube Views',
  xaxis: {
    title: 'Months',
    titlefont: {
      family: 'Courier New, monospace',
      size: 18,
      color: '#7f7f7f'
    }
  },
  yaxis: {
    title: 'Monthly views',
    titlefont: {
      family: 'Courier New, monospace',
      size: 18,
      color: '#7f7f7f'
    }
  }
};
    var data = [trace2, trace3];
    Plotly.newPlot('myDiv', data, layout);
  </script>
